    <declare-styleable name="CrmItemView">
        <attr name="itemText" format="string" />
        <attr name="itemImage" format="reference" />
        <attr name="itemUnderline" format="boolean" />
        <attr name="itemDivide" format="boolean" />
    </declare-styleable>
